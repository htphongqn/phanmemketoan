using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Text;
using System.Windows.Forms;
using DevComponents.DotNetBar;

namespace ketoansoft.app
{
    public partial class fMain : DevComponents.DotNetBar.Metro.MetroForm
    {
        public fMain()
        {
            InitializeComponent();
        }

        #region funtion
        private void fCaiThongSo()
        {
            fCaiThongSo _from = null;
            if (_from == null || _from.IsDisposed)
            {
                _from = new fCaiThongSo();
                _from.MdiParent = this;
                _from.Show();
            }
            else
            {
                _from.Activate();
            }
        }
        #endregion

        private void fMain_Load(object sender, EventArgs e)
        {
            if(fHome._IdAction == 1)
                fCaiThongSo();
        }
    }
}