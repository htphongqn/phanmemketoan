using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Text;
using System.Windows.Forms;
using DevComponents.DotNetBar;
using ketoansoft.app.Class.Global;

namespace ketoansoft.app
{
    public partial class fHome : DevComponents.DotNetBar.Metro.MetroForm
    {
        public fHome()
        {
            InitializeComponent();
        }
        public static int _IdAction = 0;
        public fHome(int idAction)
        {
            idAction = _IdAction;
        }

        #region DoubleClick
        private void btnCfg_DoubleClick(object sender, EventArgs e)
        {
            _IdAction = Const.CAIDATTHONGSO;
            fMain f = new fMain();
            f.ShowDialog();
        }

        private void buttonItem14_DoubleClick(object sender, EventArgs e)
        {
            _IdAction = Const.DANHMUCTAIKHOAN;
            fMain f = new fMain();
            f.ShowDialog();
        }
        #endregion

        #region Form function
        private void fHome_FormClosed(object sender, FormClosedEventArgs e)
        {
            if (MessageBox.Show("Bạn có muốn đóng phần mềm không?", "Hỏi", MessageBoxButtons.YesNo, MessageBoxIcon.Question) == System.Windows.Forms.DialogResult.Yes)
            {
                Application.Exit();
            }
        }

        private void fHome_Load(object sender, EventArgs e)
        {

        }
        #endregion
    }
}